<div id="<%= dom_id book %>">
  <p>
    <strong>Title:</strong>
    <%= book.title %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= book.author_id %>
  </p>

  <p>
    <strong>Genre:</strong>
    <%= book.genre_id %>
  </p>

  <p>
    <strong>Publication date:</strong>
    <%= book.publication_date %>
  </p>

  <p>
    <strong>Isbn10:</strong>
    <%= book.isbn10 %>
  </p>

  <p>
    <strong>Isbn13:</strong>
    <%= book.isbn13 %>
  </p>

  <p>
    <strong>Copies available:</strong>
    <%= book.copies_available %>
  </p>

  <p>
    <strong>Book type:</strong>
    <%= book.book_type %>
  </p>

  <p>
    <strong>Digital url:</strong>
    <%= book.digital_url %>
  </p>

  <p>
    <strong>Shelf location:</strong>
    <%= book.shelf_location %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= book.status %>
  </p>

</div>
